{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-param-reassign": 0,
        "no-undef": 0,
        "no-prototype-builtins": 0,
        "consistent-return": 0,
        "no-unused-vars": 0,
        "no-plusplus": 0
    }
}
