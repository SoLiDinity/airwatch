{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "import/no-extraneous-dependencies": 0,
    "linebreak-style": 0,
    "camelcase": 0,
    "no-console": 0,
    "consistent-return": 0,
    "no-unused-vars": 0
  }
}
